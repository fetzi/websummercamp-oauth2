{
    "name": "fetzi/websummercamp-oauth2",
    "description": "",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "hackerboy/json-api": "^1.2",
        "illuminate/database": "^5.8",
        "league/oauth2-server": "^8.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "dev-master",
        "slim/slim": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
